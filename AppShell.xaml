<?xml version="1.0" encoding="UTF-8" ?>
<Shell
 x:Class="Notas.AppShell"
 xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
 xmlns:views="clr-namespace:Notas.Views"
 Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <ShellContent
 Title="Notas"
 ContentTemplate="{DataTemplate views:PaginaTodasNotas}"
 Icon="{OnPlatform 'icon_notes.png', iOS='icon_notes_ios.png',
MacCatalyst='icon_notes_ios.png'}" />
        <ShellContent
 Title="Sobre"
 ContentTemplate="{DataTemplate views:PaginaSobre}"
 Icon="{OnPlatform 'icon_about.png', iOS='icon_about_ios.png',
MacCatalyst='icon_about_ios.png'}" />
    </TabBar>
</Shell>